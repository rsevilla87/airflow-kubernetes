{
    "benchmarks": [
        {
            "name": "host-network",
            "workload": "network-perf",
            "command": "./run.sh",
            "env": {
                "WORKLOAD": "hostnet",
                "PAIRS": "1",
                "METADATA_COLLECTION": "false"
            }
        },
        {
            "name": "pod-network",
            "workload": "network-perf",
            "command": "./run.sh",
            "env": {
                "WORKLOAD": "pod2pod",
                "METADATA_COLLECTION": "false"
            }
        },
        {
            "name": "serviceip-network",
            "workload": "network-perf",
            "command": "./run.sh",
            "env": {
                "WORKLOAD": "pod2svc",
                "METADATA_COLLECTION": "false"
            }
        }
    ]
}
