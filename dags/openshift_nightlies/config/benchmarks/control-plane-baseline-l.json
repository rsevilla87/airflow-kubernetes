{
    "benchmarks": [
        {
            "name": "cluster-density4k",
            "workload": "kube-burner",
            "trigger_rule": "all_done",
            "command": "./run.sh",
            "env": {
                "WORKLOAD": "cluster-density",
                "JOB_ITERATIONS": "4000",
                "JOB_TIMEOUT": "18000",
                "STEP_SIZE": "30s",
                "QPS": "20",
                "BURST": "20",
                "LOG_LEVEL": "info",
                "PLATFORM_ALERTS": "false",
                "CLEANUP_WHEN_FINISH": "true",
                "CLEANUP": "true",
                "CHURN": "true",
                "CHURN_DURATION": "1h",
                "CHURN_DELAY": "2m",
                "CHURN_PERCENT": "10",
                "MUST_GATHER_EACH_TASK": "true"
            }
        },
        {
            "name": "cluster-density-v2",
            "workload": "kube-burner",
            "trigger_rule": "all_done",
            "custom_cmd": "kube-burner ocp cluster-density --uuid=${UUID} --iterations=3000 --timeout=5h --es-server=${ES_SERVER} --es-index=ripsaw-kube-burner --gc=false --churn=true"
        }
    ]
}
